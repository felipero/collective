  "description": "An app to register collections",
  "main": "index.js",
  "author": "Felipe Rodrigues <felipe@crafters.cc>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config config/webpack.prod.js --optimize-minimize --env.production",
    "build:stats": "webpack --env.production --config config/webpack.prod.js --optimize-minimize --json > stats.json",
    "watch": "webpack --watch --config config/webpack.prod.js --optimize-minimize --env.production",
    "start": "webpack-dev-server --color --config config/webpack.dev.js",
    "start:stats": "webpack --config config/webpack.dev.js --json > start-stats.json",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' --ext .js,.jsx app test/unit/specs",
    "stylelint": "stylelint --config ./node_modules/stylelint-config-recommended app/stylesheets/**/*.css ./app/stylesheets/**/*.scss"
  },
  "dependencies": {
    "material-design-lite": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-webpack-plugin": "^2.30.1",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^1.5.2",
    "node-sass": "^4.7.2",
    "optimize-js-plugin": "^0.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended": "^2.0.1",
    "stylelint-webpack-plugin": "^0.9.0",
    "svg-url-loader": "^2.3.1",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1",
    "workbox-webpack-plugin": "^3.0.0-alpha.4"
  }
